/dts-v1/;
#include "common/tegra194-p3668-common.dtsi"
#include "common/tegra194-p3509-0000-a00.dtsi"

/ {
	nvidia,dtsfilename = __FILE__;
	nvidia,dtbbuildtime = __DATE__, __TIME__;

	compatible = "nvidia,p3449-0000+p3668-0001", "nvidia,p3509-0000+p3668-0001", "nvidia,tegra194";

	model = "NVIDIA Jetson Xavier NX EAC-2000";
	nsdk_version = "v0.0.0.6";

	sdhci@3400000 {
		status = "disabled";
	};

	/********************** EAC-2000 SETTING **********************/
	/* Disable Navier NX original ethernet port */
	ethernet@2490000 {
		status = "disabled";
	};

	/* For Audio i2s0 (CA) <--> dap5 */
	i2s@2901400 {
		status = "okay";
	};

	/* Audio I2S5 (SOM) pin mux */
	pinmux@2430000 {
		compatible = "nvidia,tegra194-pinmux";
		reg = <0x0 0x2430000 0x0 0x17000 0x0 0xc300000 0x0 0x4000>;
		#gpio-range-cells = <0x3>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <0x7>;
		linux,phandle = <0xb3>;
		phandle = <0xb3>;

		header-40pin-pinmux {
			linux,phandle = <0x7>;
			phandle = <0x7>;

			pin12 {
				nvidia,pins = "dap5_sclk_pt5";
				nvidia,function = "i2s5";
				nvidia,pull = <0x1>;
				nvidia,tristate = <0x0>;
				nvidia,enable-input = <0x1>;
			};

			pin35 {
				nvidia,pins = "dap5_fs_pu0";
				nvidia,function = "i2s5";
				nvidia,pull = <0x1>;
				nvidia,tristate = <0x0>;
				nvidia,enable-input = <0x1>;
			};

			pin38 {
				nvidia,pins = "dap5_din_pt7";
				nvidia,function = "i2s5";
				nvidia,pull = <0x1>;
				nvidia,tristate = <0x1>;
				nvidia,enable-input = <0x1>;
			};

			pin40 {
				nvidia,pins = "dap5_dout_pt6";
				nvidia,function = "i2s5";
				nvidia,pull = <0x1>;
				nvidia,tristate = <0x0>;
				nvidia,enable-input = <0x0>;
			};
		};
	};
	/**************************************************************/
};
